{
  "name": "to-do-list",
  "version": "1.0.0",
  "description": "Basic to do list app using react, redux.",
  "main": "index.js",
  "scripts": {
    "env:dev": "cross-env PROJECT_ENV=dev",
    "env:prod": "cross-env PROJECT_ENV=prod",
    "env:test": "cross-env PROJECT_ENV=test",
    "test": "jest --runInBand",
    "test:w": "jest --watch",
    "build:dev": "npm run clear && npm run env:dev webpack",
    "build:prod": "npm run clear && npm run env:prod webpack",
    "start": "npm run env:dev webpack-dev-server --open",
    "start:prod": "npm run env:prod webpack-dev-server --open",
    "clear": "rimraf dist",
    "precommit": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exoslav/to-do-list.git"
  },
  "keywords": [
    "todo",
    "list",
    "react",
    "redux"
  ],
  "author": "Martin Nikl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exoslav/to-do-list/issues"
  },
  "homepage": "https://github.com/exoslav/to-do-list#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^22.4.0",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
